{
  "name": "r33-base-web",
  "version": "0.0.1",
  "author": "snoodleking <snoodleking@gmail.com>",
  "repository": "https://github.com/duncan-iaria/r33-base-web",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "src",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc.json",
    ".gitignore",
    ".prettierignore",
    ".prettierrc",
    "LICENSE",
    "package.json",
    "tsconfig.json"
  ],
  "scripts": {
    "clean": "shx rm -rf dist .parcel-cache",
    "build": "yarn clean && npm run copy-assets && parcel build src/index.html",
    "start": "yarn clean && npm run copy-assets && parcel src/index.html",
    "copy-assets": "cp -R ./src/assets/build ./dist/",
    "start-windows": "yarn clean && npm run copy-assets-windows && parcel src/index.html",
    "copy-assets-windows": "xcopy .\\src\\assets\\build\\ .\\dist\\ /e",
    "fmt": "prettier --write '{*,**/*}.{js,ts,jsx,tsx,json}'",
    "lint": "eslint . && prettier --check '{*,**/*}.{js,ts,jsx,tsx,json}'",
    "lint:fix": "eslint --fix . && yarn fmt",
    "nuke": "shx rm -rf node_modules yarn.lock",
    "deploy-qa": "yarn clean && npm run copy-assets && parcel build src/index.html && scp -r dist/* snoodleking@leaningpines.io:/var/www/r33.leaningpines.io/html/."
  },
  "dependencies": {
    "@nfteyez/sol-rayz-react": "^0.10.2",
    "@solana/wallet-adapter-base": "^0.9.3",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/wallet-adapter-react-ui": "^0.9.5",
    "@solana/wallet-adapter-wallets": "^0.15.2",
    "@solana/web3.js": "^1.31.0",
    "http-proxy-middleware": "^2.0.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-loader-spinner": "^6.0.0-0",
    "react-toastify": "^8.2.0",
    "react-unity-webgl": "^8.7.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.2.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^17.0.17",
    "@types/prettier": "^2.4.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "parcel": "^2.3.1",
    "postcss": "^8.4.11",
    "postcss-modules": "^4.3.1",
    "prettier": "^2.5.1",
    "shx": "^0.3.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "proxy": "http://localhost:3000"
}
